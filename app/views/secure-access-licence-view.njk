{% from "govuk/components/back-link/macro.njk" import govukBackLink %}

{% extends '_layout.njk' %}

{% set pageHeading = "Secure Access Licence" %}

{% block pageTitle %}{{ serviceName }}
{% endblock %}

{% block beforeContent %}
{{ super() }}
  <div class="govuk-breadcrumbs">
    <ol class="govuk-breadcrumbs__list">
      <li class="govuk-breadcrumbs__list-item">
        <a class="govuk-breadcrumbs__link" href="{% if auth.isAuthenticated %}/cdo/search/basic{% else %}/{% endif %}">Home</a>
      </li>
    </ol>
  </div>
{% endblock %}

{% block content %}

  <div class="govuk-grid-row">
    <div class="govuk-grid-column-two-thirds">

        <h1 class="govuk-heading-xl">
          Secure Access Licence
        </h1>

        {% include "./secure-access-licence-content.njk" %}

    </div>
  </div>

{% endblock %}